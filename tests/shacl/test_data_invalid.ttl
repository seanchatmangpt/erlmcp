    tcps:demandSignal "Test invalid work order" ;
    tcps:bucket "performance" ;  # Invalid: must be reliability/security/cost/compliance
    tcps:priority 5 ;
    tcps:status "pending" ;
    tcps:createdAt "2026-01-26T10:50:00Z"^^xsd:dateTime .

# INVALID: WorkOrder with invalid status
<http://taiea.io/workorder/wo-invalid-002>
    a tcps:WorkOrder ;
    tcps:demandSignal "Test invalid status" ;
    tcps:bucket "security" ;
    tcps:priority 3 ;
    tcps:status "cancelled" ;  # Invalid: must be pending/in_progress/completed/failed
    tcps:createdAt "2026-01-26T10:55:00Z"^^xsd:dateTime .

# INVALID: WorkOrder with priority out of range
<http://taiea.io/workorder/wo-invalid-003>
    a tcps:WorkOrder ;
    tcps:demandSignal "Test invalid priority" ;
    tcps:bucket "cost" ;
    tcps:priority 15 ;  # Invalid: must be 1-10
    tcps:status "pending" ;
    tcps:createdAt "2026-01-26T11:00:00Z"^^xsd:dateTime .

# INVALID: Receipt with invalid stage name
<http://taiea.io/receipt/invalid-receipt-001>
    a tcps:Receipt ;
    tcps:stageName "deployment" ;  # Invalid: not a TCPS stage
    tcps:timestamp "2026-01-26T11:05:00Z"^^xsd:dateTime ;
    tcps:status "pass" ;
    tcps:evidenceData "Invalid stage receipt" ;
    tcps:relatedSKU <http://taiea.io/sku/invalid-002> .

# INVALID: Receipt with empty evidence data
<http://taiea.io/receipt/invalid-receipt-002>
    a tcps:Receipt ;
    tcps:stageName "test" ;
    tcps:timestamp "2026-01-26T11:10:00Z"^^xsd:dateTime ;
    tcps:status "pass" ;
    tcps:evidenceData "" ;  # Invalid: must be non-empty
    tcps:relatedSKU <http://taiea.io/sku/invalid-004> .

# INVALID: Receipt missing required SKU reference
<http://taiea.io/receipt/invalid-receipt-003>
    a tcps:Receipt ;
    tcps:stageName "compile" ;
    tcps:timestamp "2026-01-26T11:15:00Z"^^xsd:dateTime ;
    tcps:status "pass" ;
    tcps:evidenceData "Build completed" .
    # Missing tcps:relatedSKU

# INVALID: Compilation receipt with errors
<http://taiea.io/receipt/invalid-compile-001>
    a tcps:Receipt ;
    tcps:stageName "compile" ;
    tcps:timestamp "2026-01-26T11:20:00Z"^^xsd:dateTime ;
    tcps:status "fail" ;
    tcps:evidenceData "Compilation failed with errors" ;
    tcps:relatedSKU <http://taiea.io/sku/invalid-005> ;
    tcps:buildOutput "3 modules failed to compile" ;
    tcps:errorCount 3 ;  # Invalid: must be 0
    tcps:warningCount 5 .

# INVALID: Test receipt with low pass rate
<http://taiea.io/receipt/invalid-test-001>
    a tcps:Receipt ;
    tcps:stageName "test" ;
    tcps:timestamp "2026-01-26T11:25:00Z"^^xsd:dateTime ;
    tcps:status "fail" ;
    tcps:evidenceData "Tests failed quality gate" ;
    tcps:relatedSKU <http://taiea.io/sku/invalid-005> ;
    tcps:testResults "50 tests passed, 50 failed" ;
    tcps:passRate 50.0 ;  # Invalid: must be >= 80%
    tcps:coverage 75.0 ;  # Invalid: must be >= 80%
    tcps:totalTests 100 ;
    tcps:failedTests 50 .

# INVALID: Release receipt with invalid hash format
<http://taiea.io/receipt/invalid-release-001>
    a tcps:Receipt ;
    tcps:stageName "release" ;
    tcps:timestamp "2026-01-26T11:30:00Z"^^xsd:dateTime ;
    tcps:status "pass" ;
    tcps:evidenceData "Release artifact created" ;
    tcps:relatedSKU <http://taiea.io/sku/invalid-005> ;
    tcps:artifactPath "file:///tmp/artifact.tar.gz" ;
    tcps:deterministicHash "abc123" ;  # Invalid: must be 64 hex chars (SHA-256)
    tcps:artifactSize 1024 .

# INVALID: Release receipt with invalid artifact path
<http://taiea.io/receipt/invalid-release-002>
    a tcps:Receipt ;
    tcps:stageName "release" ;
    tcps:timestamp "2026-01-26T11:35:00Z"^^xsd:dateTime ;
    tcps:status "pass" ;
    tcps:evidenceData "Release artifact created" ;
    tcps:relatedSKU <http://taiea.io/sku/invalid-005> ;
    tcps:artifactPath "/tmp/artifact.tar.gz" ;  # Invalid: must have URI scheme
    tcps:deterministicHash "a1b2c3d4e5f6a7b8c9d0e1f2a3b4c5d6e7f8a9b0c1d2e3f4a5b6c7d8e9f0a1b2" ;
    tcps:artifactSize 1024 .

# INVALID: Publish receipt with smoke test failure
<http://taiea.io/receipt/invalid-publish-001>
    a tcps:Receipt ;
    tcps:stageName "publish" ;
    tcps:timestamp "2026-01-26T11:40:00Z"^^xsd:dateTime ;
    tcps:status "fail" ;
    tcps:evidenceData "Smoke test failed" ;
    tcps:relatedSKU <http://taiea.io/sku/invalid-005> ;
    tcps:marketplaceUrl "https://marketplace.taiea.io/test"^^xsd:anyURI ;
