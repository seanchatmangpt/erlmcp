    tcps:entitlementGateActive true .

# INVALID: Publish receipt with entitlement gate inactive
<http://taiea.io/receipt/invalid-publish-002>
    a tcps:Receipt ;
    tcps:stageName "publish" ;
    tcps:timestamp "2026-01-26T11:45:00Z"^^xsd:dateTime ;
    tcps:status "pass" ;
    tcps:evidenceData "Published without entitlement gate" ;
    tcps:relatedSKU <http://taiea.io/sku/invalid-005> ;
    tcps:marketplaceUrl "https://marketplace.taiea.io/test"^^xsd:anyURI ;
    tcps:smokeTestResult "pass" ;
    tcps:entitlementGateActive false .  # Invalid: must be true

# INVALID: Andon event with short failure reason
<http://taiea.io/andon/invalid-andon-001>
    a tcps:AndonEvent ;
    tcps:failureReason "Error" ;  # Invalid: minimum 10 characters
    tcps:affectedSKU <http://taiea.io/sku/invalid-005> ;
    tcps:rootCauseAnalysis "Bad code" ;  # Invalid: minimum 20 characters
    tcps:status "open" ;
    tcps:createdAt "2026-01-26T11:20:00Z"^^xsd:dateTime ;
    tcps:severity "critical" .

# INVALID: Andon event missing affected SKU
<http://taiea.io/andon/invalid-andon-002>
    a tcps:AndonEvent ;
    tcps:failureReason "Critical failure in production pipeline" ;
