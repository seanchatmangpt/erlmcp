# ============================================================================
# INVALID TEST DATA - Should Fail SHACL Validations
# ============================================================================

# INVALID: SKU missing required fields (name, version, description)
<http://taiea.io/sku/invalid-001>
    a tcps:SKU ;
    tcps:workOrderId <http://taiea.io/workorder/wo-invalid-001> ;
    tcps:qualityGatesPassed true ;
    tcps:shippable false ;
    tcps:createdAt "2026-01-26T11:00:00Z"^^xsd:dateTime .

# INVALID: SKU with invalid version format
<http://taiea.io/sku/invalid-002>
    a tcps:SKU ;
    tcps:name "bad-version-sku" ;
    tcps:version "v1.0" ;  # Should be "1.0.0"
    tcps:description "SKU with invalid semantic version" ;
    tcps:workOrderId <http://taiea.io/workorder/wo-invalid-001> ;
    tcps:hasReceipt <http://taiea.io/receipt/invalid-receipt-001> ;
    tcps:qualityGatesPassed true ;
    tcps:shippable true ;
    tcps:createdAt "2026-01-26T11:05:00Z"^^xsd:dateTime .

