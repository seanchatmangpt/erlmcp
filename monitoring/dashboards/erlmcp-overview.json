    {
      "datasource": "Prometheus",
      "description": "Overall system status",
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {"options": {"0": {"text": "Healthy"}, "1": {"text": "Degraded"}, "2": {"text": "Unhealthy"}}, "type": "value"},
            {"options": {"A": {"text": "Active"}, "I": {"text": "Inactive"}}, "type": "value"},
            {"options": {"N": {"text": "No Data"}, "Y": {"text": "Yes"}}, "type": "value"}
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1},
              {"color": "red", "value": 2}
            ]
          }
        }
      },
      "gridPos": {"h": 8, "w": 8, "x": 0, "y": 1},
      "id": 2,
      "options": {
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "pluginVersion": "7.5.0",
      "targets": [
        {
          "expr": "erlmcp_system_health_status",
          "instant": true,
          "interval": "30s",
          "legendFormat": "{{component}}"
        }
      ],
      "title": "System Health",
      "type": "stat"
    },
